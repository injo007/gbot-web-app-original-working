# Development environment settings

# API Configuration
VITE_API_BASE_URL=/api
VITE_API_TIMEOUT=30000

# Feature Flags
VITE_ENABLE_MOCK_API=false
VITE_ENABLE_DEBUG_TOOLS=true
VITE_ENABLE_ERROR_REPORTING=false

# Authentication
VITE_AUTH_COOKIE_NAME=gbot_session
VITE_AUTH_COOKIE_DOMAIN=localhost

# Application Settings
VITE_APP_NAME=GBot Web App (Dev)
VITE_APP_VERSION=$npm_package_version
VITE_APP_ENVIRONMENT=development

# Performance
VITE_ENABLE_QUERY_CACHE=true
VITE_QUERY_STALE_TIME=300000 # 5 minutes
VITE_QUERY_CACHE_TIME=900000 # 15 minutes

# Monitoring
VITE_ENABLE_PERFORMANCE_MONITORING=false
VITE_ENABLE_ERROR_LOGGING=true
VITE_LOG_LEVEL=debug
